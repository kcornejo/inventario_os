                            {#                            <h2 class="brand-text font-size-18">WebPos</h2>#}
                        </div>

                        {% if error %}
                            <div class="alert alert-danger">{{ error.messageKey|trans(error.messageData, 'security') }}</div>
                        {% endif %}
                        {% for flashMessage in app.session.flashbag.get('error') %}
                            <div class="alert alert-danger">{{ flashMessage }}</div>
                        {% endfor %}
                        <form onsubmit="vel_submit('#form_login');
                                return false;" action="{{ path("webpos_frontend_valida_sesion") }}" method="post" id="form_login">
                            <input type="hidden" name="_csrf_token" value="{{ csrf_token }}" />
                            <div class="form-group form-material floating" data-plugin="formMaterial">
                                <input type="text" class="form-control" name="_username"  id="username" autocomplete="off" value="{{ last_username }}" required="required" />
                                <label class="floating-label">Usuario</label>
                            </div>
                            <div class="form-group form-material floating" data-plugin="formMaterial">
                                <input type="password" class="form-control" autocomplete="off" name="_password" id="password" required="required" />
                                <label class="floating-label">Clave</label>
                            </div>
                            <span class="help-inline">
                                Usa tus Credenciales de Active Directory
                            </span>
                            <button type="submit" id="_submit"  name="_submit" class="btn btn-primary btn-block btn-lg mt-40">Registro</button>
                        </form>
                    </div>
                </div>
                <footer class="page-copyright page-copyright-inverse">
